{
  "name": "melisplatform/melis-dbdeploy",
  "description": "Melis Platform Database deployment module",
  "type": "melisplatform-module",
  "license": "OSL-3.0",
  "homepage": "https://github.com/melisplatform/melis-dbdeploy",
  "support": {
    "source": "https://github.com/melisplatform/melis-dbdeploy",
    "issues": "https://github.com/melisplatform/melis-dbdeploy/issues"
  },
  "keywords": [
    "cms",
    "melis",
    "zf2",
    "module"
  ],
  "extra": {
    "module-name": "MelisDbDeploy",
    "dbdeploy": true
  },
  "require": {
    "php": "^5.5 || ^7.0",
    "melisplatform/melis-core": "~2.2.7",
    "zendframework/zendframework": "2.5.3",
    "phing/phing": "~2.16.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "psy/psysh": "@stable"
  },
  "autoload": {
    "psr-4": {
      "MelisDbDeploy\\": "src/"
    }
  },
  "scripts": {
    "post-update-cmd": "MelisDbDeploy\\Module::run"
  }
}
